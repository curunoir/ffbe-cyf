{
    "name": "laravel/laravel",
    "minimum-stability": "dev",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/bluemountainteam/translation"
        },
        {
            "type": "vcs",
            "url": "https://github.com/bluemountainteam/bootform"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "dlouvard/bootformlaravel": "dev-master",
        "laravel/framework": "5.4.*",
        "laravel/tinker": "~1.0",
        "laravelcollective/html": "~5.4",
        "stolz/assets": "^0.1.9",
        "vinkla/hashids": "^3.1",
        "bluemountainteam/translation": "master@dev",
        "stolz/assets": "^0.1.9",
        "bluemountainteam/bootform": "master@dev"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "barryvdh/laravel-debugbar": "^2.4",
        "barryvdh/laravel-ide-helper": "^2.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.7"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        },
        "files": [ "app/Http/helpers.php" ]
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
